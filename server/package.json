{
  "name": "server",
  "version": "1.0.0",
  "description": "Portfolio Graphql server",
  "main": "build/cjs/index.cjs.js",
  "module": "build/es/index.es.js",
  "types": "build/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./build/cjs/index.cjs.js",
      "import": "./build/es/index.es.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup --config rollup.config.js --environment NODE_ENV:production",
    "build:dev": "rollup --config rollup.config.js --environment NODE_ENV:development",
    "watch": "rollup --config rollup.config.js --watch",
    "dev": "yarn watch --environment NODE_ENV:development",
    "predev": "yarn sync",
    "start:ts": "ts-node src/index.ts",
    "start": "node --register dotenv/config build/cjs/index.cjs.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "sync": "yarn typeorm schema:sync",
    "migrate:up": "yarn typeorm migration:run",
    "migrate:down": "yarn typeorm migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "apollo-datasource": "^3.3.0",
    "apollo-server-cache-redis": "^3.3.0",
    "apollo-server-core": "^3.6.1",
    "apollo-server-express": "^3.6.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "graphql": "^16.2.0",
    "graphql-fields": "^2.0.3",
    "graphql-upload": "^13.0.0",
    "ioredis": "^4.28.3",
    "join-monster": "^3.0.4",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-graphql": "^1.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-run": "^2.1.0",
    "@types/express": "^4.17.13",
    "@types/graphql-fields": "^1.3.4",
    "@types/graphql-upload": "^8.0.9",
    "@types/ioredis": "^4.28.7",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.8",
    "apollo-server-caching": "^3.3.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "rollup": "^2.63.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-tsconfig-paths": "^1.1.6",
    "rollup-plugin-typescript2": "^0.31.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
